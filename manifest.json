{
    "name": "Bootcamp Spot Percentage Display",
    "version": "1.2.1",
    "author": "Dominic Smith",
    "description": "*Developed for UCF coding bootcamp students",
    "permissions": ["tabs", "*://*.bootcampspot.com/*"],
    "content_scripts": [
      {
        "matches": ["*://*.bootcampspot.com/*"],
        "js": ["percentage.js"]
      }
    ],
    "browser_action": {
        "default_icon": "icon48.png"
    },
    "icons" : {
        "48":"icon48.png"
    },
    "manifest_version" : 2
}